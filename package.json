{
  "name": "cgen-site",
  "private": true,
  "description": "Candee Generations Website",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 3002",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "concurrently -n type,site -c red,blue \"tsc-watch --noClear\" \"npm run develop\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.21.9",
    "gatsby-plugin-typescript": "^2.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "2.0.5",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint",
    "*.{js,md,ts,tsx,css,sass,less,graphql,yml,yaml,json}": "npm run format"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/candeegenerations/cgen-site"
  },
  "bugs": {
    "url": "https://github.com/candeegenerations/cgen-site/issues"
  }
}
